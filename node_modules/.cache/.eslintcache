 return (\n    <div className=\"result\">\n      <img src=\"https://cdn-icons-png.flaticon.com/512/2278/2278992.png\" />\n      <h2>you guessed {correct} answers from {questions.length}</h2>\n      <a href=\"/\">\n        <button>Try again</button>\n      </a>\n    </div>\n  );\n}\n\nfunction Game({ step, question, onClickVariant }) {\n  \n  const percentage = Math.round(step/question.variants.length * 100);\n\n  return (\n    <>\n      <div className=\"progress\">\n        <div style={{ width: `${percentage}%` }} className=\"progress__inner\"></div>\n      </div>\n      <h1>{question.title}</h1>\n      <ul >\n        {question.variants.map((x, index) => (<li onClick={()=>onClickVariant(index, percentage)} key={x}>{x}</li>))}\n      </ul>\n    </>\n  );\n}\n\nfunction App() {\n  const [step, setStep] = React.useState(0)\n\n  const [correct, setCorrect] = React.useState(0)\n\n  const question = questions[step];\n\n  const onClickVariant = (index, percentage) => {\n    console.log(step, index, percentage)    \n    setStep(step + 1)\n    \n    if (index === question.correct) { \n      setCorrect(correct+1)\n    }\n  }\n\n  return (\n    <div className=\"App\">\n      {step !== questions.length ?\n        (<Game\n          step={step}\n          question={question}\n          onClickVariant={onClickVariant}\n        />) :\n        (<Result\n          correct={correct}\n        />)}\n    </div>\n  );\n}\n\nexport default App;\n",{"ruleId":"26","severity":1,"message":"27","line":31,"column":7,"nodeType":"28","endLine":31,"endColumn":76},{"ruleId":"26","severity":1,"message":"27","line":31,"column":7,"nodeType":"28","endLine":31,"endColumn":76},"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement"]